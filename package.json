{
  "name": "typescript-node-starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "jest --forceExit --watchAll --verbose",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build": "npm run build-ts && npm run lint",
    "watch-node": "nodemon dist/server.js",
    "start": "node dist/server.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node,Jest\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\" \"npm run watch-test\""
  },
  "author": "Eddy Lazar",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.20",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "concurrently": "^4.1.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/express": "^4.17.1",
    "express": "^4.17.1"
  }
}
